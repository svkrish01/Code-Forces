//1046 - A. In the Dream

#include <iostream>

using namespace std;

int main()
{
    int Cases;
    int A, B, C, D;
    
    cin >> Cases;
    
    for(int X = 0; X < Cases; X++)
    {
        cin >> A >> B >> C >> D;
        
        string Answer = (max(A, B) > 2 * (min(A, B) + 1) || max(C - A, D - B) > 2 * (min(C - A, D - B) + 1)) ? "NO" : "YES";
        
        cout << Answer << "\n";
    }
}
